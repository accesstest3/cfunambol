<!ELEMENT dataset (
    FNBL_LAST_SYNC,
    FNBL_PIM_LISTENER_REGISTRY,
    FNBL_PUSH_LISTENER_REGISTRY*)>

<!ELEMENT FNBL_LAST_SYNC EMPTY>
<!ATTLIST FNBL_LAST_SYNC
    principal CDATA #REQUIRED
    sync_source CDATA #REQUIRED
    start_sync CDATA #IMPLIED
>

<!ELEMENT FNBL_PIM_LISTENER_REGISTRY EMPTY>
<!ATTLIST FNBL_PIM_LISTENER_REGISTRY
    id CDATA #REQUIRED
    username CDATA #IMPLIED
    push_contacts CDATA #IMPLIED
    push_calendars CDATA #IMPLIED
    push_notes CDATA #IMPLIED
>

<!ELEMENT FNBL_PUSH_LISTENER_REGISTRY EMPTY>
<!ATTLIST FNBL_PUSH_LISTENER_REGISTRY
    id CDATA #REQUIRED
    period CDATA #IMPLIED
    active CDATA #IMPLIED
    last_update CDATA #IMPLIED
    status CDATA #IMPLIED
    task_bean_file CDATA #IMPLIED
>
