<!--
    Rememeber to run mvn assembly:unpack first!
-->
<assembly>
  <id>jibx</id>
  <formats>
    <format>zip</format>
  </formats>
  <fileSets>
    <fileSet>
      <includes>
        <include>*.txt</include>
        <include>LICENSE.txt</include>
        <include>LICENSES/</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>target</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>*.jar</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
